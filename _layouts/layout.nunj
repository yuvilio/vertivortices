<!doctype html>
<html class="no-js" lang="en">
  <head>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{ misc.site_name }} | {{ title }}</title>
    <meta name="description" content="{{metatag_description}}">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {% if dev_env.env %}
      {% include "partials/header-local.nunj" %}
      <link rel="stylesheet" href="/assets/css/main.css">
    {% else %}
      <link href="https://fonts.googleapis.com/css?family=Titillium+Web" rel="stylesheet">
      <link href="https://fonts.googleapis.com/css?family=Caudex" rel="stylesheet">
      <link rel="stylesheet" href="/{{ prod_env.sitename }}/assets/css/main.css">
    {% endif %}


    {% block header_scripts %}{% endblock %}

    {#
      favicon:
      <link rel="apple-touch-icon" href="apple-touch-icon.png">
      <!-- Place favicon.ico in the root directory -->
    #}


  </head>

  <body class="">


    <header>

      {% include "partials/header-nav.nunj" %}

    </header>


    {% block content %}{% endblock %}

    {# general scripts #}

    {# sitewide js #}
    {% if dev_env.env %}
      <script src="/assets/js/main.js"></script>
    {% else %}
      <script src="/{{ prod_env.sitename }}/assets/js/main.js"></script>
    {% endif %}


    <!-- test -->

    {# page specific scripts #}
    {% block footer_scripts %}{% endblock %}

  </body>

</html>
