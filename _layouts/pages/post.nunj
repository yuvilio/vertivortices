{# post collection #}
{% extends "layout.nunj" %}

{% block content %}
  <div class="main-wrap">

    <div class="row">
      <div class="small-12">
        <h1>{{title}} </h1>

        <div class="meta">
          {% for tag in tags %}
              <a href="/tag/{{tag.slug}}">{{tag.name}}</a>
          {% endfor %}
        </div>

        {{contents}}
      </div>
    </div>


  </div>
{% endblock %}
