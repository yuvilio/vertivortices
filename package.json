{
  "name": "vertivortices",
  "version": "0.1.0",
  "description": "",
  "main": "build/metalsmith.js",
  "engines": {
    "node": "6.9.0"
  },
  "scripts": {
    "start": "npm run browser_sync & NODE_ENV=development DEBUG=metalsmith:* ./node_modules/nodemon/bin/nodemon.js --exec 'npm run lint && npm run build:dev && browser-sync reload'",
    "browser_sync": "browser-sync start --server dist --no-ui --no-notify",
    "build:dev": "npm run templates:dev && npm run styles:dev && npm run js:dev",
    "build": "node build/metalsmith.js && postcss -c postcss.prod.json",
    "styles:dev": "postcss -c postcss.dev.json",
    "js:dev": "./node_modules/rollup/bin/rollup -c",
    "templates:dev": "node build/metalsmith.js",
    "publish": "./node_modules/gh-pages/bin/gh-pages -d dist -b gh-pages",
    "lint": "./node_modules/standard/bin/cmd.js assets/js/**/*.js && ./node_modules/stylelint/bin/stylelint.js assets/css/*.css"
  },
  "author": "yuvilio",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015-rollup": "^3.0.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.9.1",
    "gh-pages": "^0.12.0",
    "metalsmith": "^2.3.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-excerpts": "^1.1.0",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-watch": "^1.0.3",
    "nunjucks": "^3.0.0",
    "nunjucks-date": "^1.2.0",
    "postcss": "^5.2.8",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.0.0",
    "postcss-nested": "^1.0.0",
    "rollup": "^0.39.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "tachyons": "^4.6.1"
  },
  "dependencies": {}
}
